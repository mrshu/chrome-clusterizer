{
  "name": "Tabs Clusterized",
  "version": "0.1.5",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "Tabs Clusterizer",
  "icons": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "256": "img/icon_256.png"
  },
  "browser_action": {
      "default_popup": "html/popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+C",
        "mac": "Alt+C",
        "linux": "Alt+C",
        "windows": "Alt+C"
      }
    }
  },
  "permissions": ["tabs", "http://*/", "https://*/"],
  "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
                "js/jquery.js",
                "js/scraper.js"
        ]
  }],
  "background": {
          "scripts": ["js/background.js"]
  },
  "web_accessible_resources": []
}
