{
  "name": "Tabs Clusterized",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Tabs Clusterizer",
  "icons": {
      "16": "img/icon_16.png"
  },
  "browser_action": {
      "default_popup": "html/popup.html"
  },
  "permissions": ["tabs"],
  "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
                "js/jquery.js",
                "js/scraper.js"
        ]
  }],
  "background": {
          "scripts": ["js/background.js"]
  },
  "web_accessible_resources": []
}
